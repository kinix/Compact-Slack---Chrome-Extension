{
  "name": "Compact Slack Sidebar",
  "description": "Compact Slack Sidebar",
  "version": "0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage", "https://app.slack.com/*"],
  "content_scripts": [{
    "matches": ["https://app.slack.com/*"],
    "js": ["jquery.min.js", "index.js"]
  }],
  "manifest_version": 2
}